<!DOCTYPE html>
<html>
  <head>
    <title>Super Insurance</title>
    <%- include('header'); %>
  </head>
  <body class="is-preload">
    <!-- Wrapper -->
    <div id="wrapper">
      <%- include('navbar'); %>
        <!-- Main -->
        <div id="main">
          <div class="inner">

            <table class="table align-middle mb-0 bg-white table-striped">
              <thead class="bg-light">
                <tr>
                  <th scope="col"><b>#</b></th>
                  <th scope="col"><b>Nom & Prénom</b></th>
                  <th scope="col"><b>Téléphone</b></th>
                  <th scope="col"><b>Adresse</b></th>
                  <th scope="col"><b>Email</b></th>
                  <th scope="col"><b>Token</b></th>
                  <th scope="col"><b>Premium</b></th>
                </tr>
              </thead>
              <tbody>
                <% for (quote of quotes) { %>
                <tr>
                  <th scope="row"><b><%= quote.id %></b></th>
                  <td><%= quote.name %></td>
                  <td><%= quote.phone %></td>
                  <td><%= quote.address %></td>
                  <td><%= quote.email %></td>
                  <td><%= quote.token %></td>
                  <td><%= quote.premium %> €</td>
                </tr>
                <% } %>
              </tbody>
            </table>
          </div>
        </div>
      </div>

    
    <%- include('script'); %>
  </body>
</html>